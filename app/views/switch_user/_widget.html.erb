<div class= "dm-up-main" >
<div class="form-horizontal text-md dm-up-main-static current-profile-info">
<% if SwitchUser.switch_back %>
<div class="form-group row">
  <label class="col-sm-3 control-label">Remember me:</label>
  <div class="col-sm-7">
    <%= check_box_tag "remember_user", "remember_user", provider.original_user.present?, onchange: "location.href = '#{ActionController::Base.relative_url_root || '/'}switch_user/remember_user?remember=' + encodeURIComponent(this.checked)", class: "form-control su-checkbox" %>
  </div>
</div>
<% end %>
<div class="form-group row">
  <label class="col-sm-3 control-label">Switch to user:</label>
  <div class="col-sm-7">
    <%= select_tag "switch_user_identifier", option_tags,   onchange: "location.href = '#{ActionController::Base.relative_url_root || '/'}switch_user?scope_identifier=' + encodeURIComponent(this.options[this.selectedIndex].value)", class: "#{classes} form-control", style: styles %>
  </div>
</div>
  </div>
</div>